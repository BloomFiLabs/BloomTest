{
  "name": "bloom-backtesting-framework",
  "version": "1.0.0",
  "description": "DDD Hexagonal Architecture Backtesting Framework for Bloom Vault Strategies",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "npm test -- --run && tsx src/cli.ts",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "cli": "tsx src/cli.ts",
    "backtest": "tsx src/cli.ts",
    "backtest:aave": "tsx scripts/backtest-leveraged-lending-aave.ts",
    "backtest:snx:funding": "tsx scripts/backtest-synthetix-funding-rate-capture.ts",
    "backtest:hype": "tsx scripts/fetch-hyperliquid-data.ts && tsx scripts/backtest-hyperliquid-funding.ts",
    "prepare-data": "tsx scripts/prepare-sample-data.ts",
    "fetch-data": "tsx scripts/fetch-price-data.ts"
  },
  "bin": {
    "bloom-backtest": "./dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AureliusEth/BloomTest.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/AureliusEth/BloomTest/issues"
  },
  "homepage": "https://github.com/AureliusEth/BloomTest#readme",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitest/ui": "^4.0.9",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.9"
  },
  "dependencies": {
    "@uniswap/sdk": "^3.0.3",
    "axios": "^1.13.2",
    "commander": "^14.0.2",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "ethers": "^6.15.0",
    "graphql": "^16.12.0",
    "graphql-request": "^7.3.3",
    "viem": "^2.39.0",
    "yaml": "^2.8.1",
    "zod": "^4.1.12"
  }
}
